<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".dependientes.InicioAdministrador">

    <FrameLayout
        android:id="@+id/adm_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toTopOf="@+id/adm_navigator"></FrameLayout>

    <android.support.design.widget.BottomNavigationView
        android:id="@+id/adm_navigator"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="0dp"
        android:layout_marginStart="0dp"
        android:background="?android:attr/windowBackground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:backgroundTint="@color/colorPrimary"
        app:itemBackground="@color/colorPrimary"
        app:itemIconTint="@color/textWhite"
        app:itemTextColor="@color/textWhite"
        app:menu="@menu/administrador_navigator"></android.support.design.widget.BottomNavigationView>

</android.support.constraint.ConstraintLayout>